{
  "name": "MYKN",
  "version": "0.0.1",
  "description": "My Knowledge Network",
  "permissions": [
    "bookmarks",
    "history",
    "tabs",
    "storage"
  ],
  "icons": {
    "48": "img/logo/48x48.png"
  },
  "browser_action": {
      "default_title": "MYKN",
      "default_icon": "img/logo/48x48.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}